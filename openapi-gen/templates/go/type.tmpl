{{- define "generateObject"}} struct {
    {{- range $key, $value := .Properties}}
    {{$key}} {{template "generateType" $value.Value}} `json:"{{$key}}"`
    {{- end}}
}
{{- end}}

{{- define "generateType"}}
    {{- if (eq .Type "object")}}{{- template "generateObject" .}}{{- end}}
    {{- if (eq .Type "string")}}string{{end}}
    {{- if (eq .Type "integer")}}int{{end}}
    {{- if (eq .Type "number")}}float64{{end}}
    {{- if (eq .Type "boolean")}}bool{{end}}
    {{- if (eq .Type "array")}}[]{{template "generateType" .Items.Value}}{{end}}
    {{- if (eq .Type "null")}}interface{}{{end}}
{{- end}}

type {{.Name}}{{template "generateType" .Schema}}
