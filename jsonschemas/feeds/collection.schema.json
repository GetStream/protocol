{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "collection.schema.json",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "id": { "type": "string" },
    "user_id": { "type": "string" },
    "custom": { "type": "object" },
    "created_at": { "type": "string", "format": "date-time" },
    "updated_at": { "type": "string", "format": "date-time" },
    "deleted_at": { "type": "string", "format": "date-time" }
  },
  "required": ["name", "id"]
}
